{
  "name": "shillong-teer-india",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "pwa:install": "npm install -g workbox-cli",
    "pwa:generate": "workbox generateSW workbox-config.js",
    "pwa:build": "npm run build && npm run pwa:generate",
    "apk:install": "npm install -g @bubblewrap/cli",
    "apk:build": "node scripts/build-android-apk.js"
  }
}